{
    "name": "studapart/stud-cli",
    "description": "Stud Cli DX - Jira & Git Workflow Streamliner",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "symfony/http-client": "^6.4|^7.0",
        "symfony/process": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "jolicode/castor": "^1.0.0",
        "symfony/yaml": "^6.4|^7.0",
        "stevebauman/hypertext": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "minimum-stability": "dev",
        "prefer-stable": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "phpspec/prophecy-phpunit": "^2.4"
    },
    "scripts": {
        "dump-config": "php scripts/dump-config.php"
    }
}