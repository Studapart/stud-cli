{
    "name": "studapart/stud-cli",
    "description": "Stud Cli DX - Jira & Git Workflow Streamliner",
    "type": "project",
    "license": "MIT",
    "version": "2.9.0",
    "authors": [
        {
            "name": "Pierre-Emmanuel Manteau",
            "homepage": "https://github.com/PEM-FR",
            "role": "Head of Engineering"
        }
    ],
    "support": {
        "issues": "https://github.com/studapart/stud-cli/issues",
        "source": "https://github.com/studapart/stud-cli"
    },
    "homepage": "https://github.com/studapart/stud-cli",
    "keywords": [
        "stud",
        "stud-cli",
        "jira",
        "git",
        "workflow"
    ],
    "require": {
        "symfony/http-client": "^7.3",
        "symfony/process": "^7.3",
        "symfony/console": "^7.3,<7.4",
        "symfony/string": "^7.3",
        "jolicode/castor": "^1.1.0",
        "symfony/yaml": "^7.3",
        "symfony/translation": "^7.3",
        "stevebauman/hypertext": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "minimum-stability": "dev",
        "prefer-stable": true,
        "allow-plugins": {
            "symfony/flex": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpspec/prophecy-phpunit": "^2.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "dump-config": "php scripts/dump-config.php"
    }
}