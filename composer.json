{
    "name": "studapart/stud-cli",
    "description": "A CLI tool to integrate Jira and Git workflows.",
    "type": "project",
    "license": "MIT",
    "require": {
        "symfony/http-client": "^6.4|^7.0",
        "symfony/process": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "jolicode/castor": "^1.0.0",
        "symfony/yaml": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },

    "require-dev": {
        "humbug/box": "^4.0"
    },
    "config": {
        "minimum-stability": "dev",
        "prefer-stable": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
